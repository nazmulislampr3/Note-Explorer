{
  "version": 2,
  "name": "note-explorer",
  "builds": [
    {
      "src": "server/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/index.ts"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongo_uri",
    "NODE_ENV": "production"
  }
}
